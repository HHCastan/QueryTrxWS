log4j.rootLogger=INFO, console, transfer4690

# log
log4j.appender.transfer4690=org.apache.log4j.RollingFileAppender
log4j.appender.transfer4690.File=C:/TMP/transfer4690.log
log4j.appender.transfer4690.MaxFileSize=2MB
log4j.appender.transfer4690.MaxBackupIndex=5 
log4j.appender.transfer4690.layout=org.apache.log4j.PatternLayout
log4j.appender.transfer4690.layout.ConversionPattern=<%d{dd-MMM-yyyy HH:mm:ss,SSS}> [%-5p] [%C{1}] - %m%n

log4j.appender.console=org.apache.log4j.ConsoleAppender
log4j.appender.console.Target=System.out
log4j.appender.console.layout=org.apache.log4j.PatternLayout
log4j.appender.console.layout.ConversionPattern=<%d{dd-MMM-yyyy HH:mm:ss,SSS}> [%-5p] [%C{1}] - %m%n
